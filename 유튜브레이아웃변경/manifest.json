{
    "manifest_version": 3,
    "name": "YouTube Layout Customizer",
    "version": "1.0",
    "description": "Change the layout of YouTube.",
    "permissions": ["scripting"],
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/*"],
        "css": ["styles.css"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }
  